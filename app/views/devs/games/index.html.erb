<p><a href="/">Home</a> — <a href="/devs">Developer Index</a> — <a href="/games">Games Index</a></p>
<hr class="solid">

<h1><%= @dev.name %>'s Games</h1>
    <% @dev.games.each do |game| %>
        <h3><%= game.name %></h3>
        <ul>
            <li><p>Number of Players: <%= game.players %></p></li>
            <li><p>Has Online Features: <%= game.online? %></p></li>
        </ul>
    <% end %>
<h4><a href=<%= "games/new" %> title="Add Game to Developer">Add Game to <%= @dev.name %></a></h4>
